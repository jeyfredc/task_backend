{
    "version": 2,
    "builds": [
        {
            "src": "src/**/*.ts",
            "use": "@vercel/node",
            "config": {
                "zero-config": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ]
}
